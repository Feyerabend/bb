# Conditionals and Comparisons
let age = 25;

if age >= 18 {
    print("You are an adult");
} else {
    print("You are a minor");
}

let score = 85;
if score >= 90 {
    print("Grade: A");
} else {
    if score >= 80 {
        print("Grade: B");
    } else {
        print("Grade: C or below");
    }
}

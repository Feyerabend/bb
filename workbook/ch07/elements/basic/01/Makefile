# Makefile for BASIC interpreter

.PHONY: run clean help

run:
	@python3 basic_interpreter.py

clean:
	@echo "Cleaning up.."
	@rm -rf __pycache__
	@rm -f *.pyc

help:
	@echo "Available targets:"
	@echo "  run     - Start the BASIC"
	@echo "  clean   - Remove Python cache files"
	@echo "  help    - Show this help message"
